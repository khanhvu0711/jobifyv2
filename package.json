{
  "name": "viteapp",
  "version": "1.0.0",
  "description": "[Jobify](https://jobify.live/)",
  "main": "server.js",
  "scripts": {
    "setup-project": "npm i && client && npm i",
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "nodemon --exec babel-node src/server.js",
    "client": "cd client && npm run dev",
    "dev": "concurrently --kill-others-on-fail \" npm run server\" \"npm run client ",
    "watch": "node --watch server.js",
    "populate": "node babel-node populate.js",
    "build": "babel src -d ./build",
    "start": "node build/server.js"
  },
  "keywords": [],
  "author": "Mipha",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.2",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.23.2",
    "@babel/register": "^7.22.15",
    "bcryptjs": "^2.4.3",
    "buffer": "^6.0.3",
    "concurrently": "^8.0.1",
    "cookie-parser": "^1.4.6",
    "datauri": "^4.1.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^6.8.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.2",
    "nanoid": "^3.3.6",
    "nodemailer": "^6.9.7",
    "nodemon": "^2.0.22",
    "sequelize": "^6.33.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "ejs": "^3.1.9",
    "sequelize-cli": "^6.6.1"
  }
}
